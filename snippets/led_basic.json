{
	"For Next Loop": {
		"prefix": "for",
		"body": [
			"for ${1:index} = ${2:lower} to ${3:upper}",
			"\t$0",
			"next ${1:index}"
		],
		"description": "For Next Loop"
    },
    "For Next down Loop": {
		"prefix": "ford",
		"body": [
			"for ${1:index} = ${2:upper} downto ${3:lower}",
			"\t$0",
			"next ${1:index}"
		],
		"description": "For Next Loop"
	}
}