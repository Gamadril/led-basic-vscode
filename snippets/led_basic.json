{
	"For Next Loop": {
		"prefix": "for",
		"body": [
			"for ${1:index} = ${2:lower} to ${3:upper}",
			"\t$0",
			"next ${1:index}"
		],
		"description": "For Next Loop"
    },
    "For Next down Loop": {
		"prefix": "ford",
		"body": [
			"for ${1:index} = ${2:upper} downto ${3:lower}",
			"\t$0",
			"next ${1:index}"
		],
		"description": "For Next Loop"
	},
	"If Then": {
		"prefix": "if",
		"body": "if ${1:condition} then $0",
		"description": "If Then statement"
	},
	"If Then Else": {
		"prefix": "ifelse",
		"body": "if ${1:condition} then $2 else $0",
		"description": "If Then Else statement"
	}

}